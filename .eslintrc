{
  "extends": [
    "prettier",
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "no-console": "error",
    "quotes": ["error", "single"],
    "jsx-quotes": ["error", "prefer-double"],
    "prefer-const": "error",
    "indent": ["warn", 2],
    // "max-len": ["error", {"code": 108}],
    "comma-dangle": "off",
    "semi": ["error", "always"]
  },
  "plugins": [
    "prettier"
  ]
}